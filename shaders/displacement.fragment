in vec2 UV_coords;
in vec2 UV_dismap;
in int v_texnum;
in int v_disnum;
out vec4 colour;
uniform sampler2D u_texarray[32];
void main(){
  float displacement = texture(u_texarray[v_disnum], UV_dismap).g -0.5;
  //black decreases UV, white increases UV
  colour = texture(u_texarray[v_texnum], (UV_coords + displacement));
}